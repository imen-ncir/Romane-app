{
  "name": "romane",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "pi": "cd ios && arch -x86_64 pod install --repo-update && cd ..",
    "pu": "cd ios &&  arch -x86_64 pod update && cd ..",
    "pru": "cd ios &&  arch -x86_64 pod repo update && cd ..",
    "ipm": "arch -x86_64 react-native run-ios --simulator=\"I12PM\"",
    "android": "ENVFILE=.env.staging react-native run-android",
    "android:staging": "ENVFILE=.env.staging react-native run-android",
    "ios": "ENVFILE=.env arch -x86_64 react-native run-ios --simulator=\"iPhone12Mini\"",
    "ios8": "ENVFILE=.env arch -x86_64 react-native run-ios --simulator=\"iPhone8Plus\"",
    "ios:staging": "ENVFILE=.env.staging arch -x86_64 react-native run-ios --configuration=release --simulator=\"iPhone12Mini\"",
    "start": "react-native start",
    "test": "jest",
    "pod": "cd ios && pod install && cd ..",
    "lint": "eslint --quiet --fix . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ENVFILE=.env.staging ./gradlew assembleRelease",
    "build:ios": "ENVFILE=.env.staging react-native run-ios --configuration=release --simulator=\"iPhone12Mini\"",
    "start:staging": "ENVFILE=.env.staging react-native run-ios --simulator=\"iPhone12Mini\"",
    "start:production": "ENVFILE=.env.production react-native run-ios --simulator=\"iPhone12Mini\"",
    "clean": "react-native-clean-project"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.0.0",
    "@react-native-community/google-signin": "^5.0.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/app": "^11.2.0",
    "@react-native-firebase/auth": "^11.2.0",
    "@react-native-firebase/firestore": "^11.3.3",
    "@react-native-firebase/storage": "^11.2.0",
    "@react-navigation/bottom-tabs": "^5.11.8",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "@stripe/stripe-react-native": "^0.1.1",
    "@types/react": "^17",
    "@types/unique-push-id": "^1.0.0",
    "add": "^2.0.6",
    "axios": "^0.21.1",
    "d3-shape": "^2.1.0",
    "prettier": "^2.2.1",
    "react": "17.0.1",
    "react-hook-form": "^7.0.0",
    "react-native": "0.64.0",
    "react-native-audio-recorder-player": "^2.6.2",
    "react-native-check-box": "^2.1.7",
    "react-native-circular-progress": "^1.3.7",
    "react-native-config": "^1.4.2",
    "react-native-fbsdk": "^3.0.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-gifted-chat": "^0.16.3",
    "react-native-image-crop-picker": "^0.36.0",
    "react-native-image-resizer": "^1.4.4",
    "react-native-pager-view": "^5.1.2",
    "react-native-paper": "^4.7.2",
    "react-native-reanimated": "^2.1.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.0.0",
    "react-native-step-indicator": "^1.0.3",
    "react-native-svg": "^12.1.0",
    "react-native-swiper": "^1.6.0-rc.3",
    "react-native-tab-view": "^3.0.0",
    "react-native-toast-message": "^1.4.9",
    "react-native-vector-icons": "^8.1.0",
    "react-native-webview": "11.4.5",
    "recoil": "^0.2.0",
    "rn-range-slider": "^2.1.1",
    "unique-push-id": "^1.0.2",
    "yarn": "^1.22.10",
